<nodeDefs>

<!-- The node server control Node-->
  <nodeDef id="ctl" nls="ctl">
    <sts>
      <st id="ST" editor="bool" />
      <st id="GV0" editor="errors" />
      <st id="GV1" editor="bool" />
      <st id="GV2" editor="bool" />
      <st id="GV3" editor="bool" />
      <st id="GV4" editor="bool" />
      <st id="GV5" editor="running" />
	  </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="COMMAND">
          <p id="DEVICE" editor="rainDelay" />
          <p id="BUTTON" editor="rainDelay" />
          <p id="CODE" editor="rainDelay" />
          <p id="CONNECTOR" editor="rainDelay" />
          <p id="REPETE" editor="rainDelay" />
        </cmd>
        <cmd id="STOP" />
          <p id="CONNECTOR" editor="rainDelay" />
      </accepts>
    </cmds>
  </nodeDef>



 


</nodeDefs>

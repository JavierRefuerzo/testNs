<editors>
	<!-- Boolean -->
	<editor id="bool">
		<range uom="2" subset="0,1" />
	</editor>
	<editor id="code">
		<range uom="25" subset="0,1" nls="CODE"/>
	</editor>
	<editor id="connector">
		<range uom="25" subset="0,1,3" nls="CONNECTOR" />
	</editor>
	<!-- this should be an encoded editor to prevent rewirtes of ditor file-->
	<editor id="device">
		<range uom="25" min="0" max="32767" prec="0"/>
	</editor>
	<editor id="errors">
		<range uom="25" min="0" max="6" prec="0" nls="ERRORS" /> 
	</editor>
	<editor id="repete">
		<range uom="25" min="0" max="50" prec="0" /> 
	</editor>
	<!-- this should be an encoded editor to prevent rewirtes of ditor file-->
    <editor id="button">
        <range uom="25" min="0" max="32767" />
    </editor>
</editors>
